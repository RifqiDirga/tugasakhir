          <!-- SECTION 3 -->
          <section class="berwarna" id="about">
            <div
              id="testimonials-carousel"
              class="carousel slide"
              data-bs-ride="false"
            >
              <div class="carousel-inner">
                <div class="carousel-item active container-fluid">
                  <h2 class="testimonial-text">
                    (小説を音楽にする) "We make novel into music."
                  </h2>
                  <img
                    class="testimonial-img"
                    src="/images/Ayase_icon.png"
                    alt="ayase-profile"
                  />
                  <em>- motto YOASOBI -</em>
                </div>
                <div class="carousel-item container-fluid">
                  <h2 class="testimonial-text">
                    "Everything is necessary now. You don't have to worry about comparing yourself to others."
                  </h2>
                  <img
                    class="testimonial-img"
                    src="images/Ikura_icon.png"
                    alt="note-profile"
                  />
                  <em>- motto Ikura -</em>
                </div>
                <div class="carousel-item container-fluid">
                  <h2 class="testimonial-text">
                    (Yoasobi) Night life
                  </h2>
                  <img
                    class="testimonial-img"
                    src="images/happiness.png"
                    alt="happiness-profile"
                  />
                  <em>- Yoasobi -</em>
                </div>
              </div>
      
              <button
                class="carousel-control-prev"
                type="button"
                data-bs-target="#testimonials-carousel"
                data-bs-slide="prev"
              >
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button
                class="carousel-control-next"
                type="button"
                data-bs-target="#testimonials-carousel"
                data-bs-slide="next"
              >
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
